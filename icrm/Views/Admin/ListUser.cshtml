                        <div class="card-header">
                            <h6 class="slim-card-title">Ticket List</h6>
                        </div>
                        <!-- card-header -->
                        <div class="table-responsive">
                            <table class="table mg-b-0 tx-13">
                                <thead>
                                    <tr class="tx-10">
                                        <th class="wd-10p pd-y-5">Employee ID</th>
                                        <th class="pd-y-5">FirstName</th>
                                        <th class="pd-y-5">Last Name</th>
                                        <th class="pd-y-5">Email</th>
                                        <th class="pd-y-5">Phone Number</th>
                                        <th class="pd-y-5">Status</th>
                                       
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td class="pd-l-20">
                                                @Html.DisplayFor(modelItem => item.EmployeeId)
                                            </td>
                                            <td>
                                                <a href="default.htm" class="tx-inverse tx-14 tx-medium d-block"> @Html.DisplayFor(modelItem => item.FirstName)</a>
                                                
                                            </td>
                                            <td class="tx-12 text-center">
                                                @Html.DisplayFor(modelItem => item.LastName)
                                            </td>
                                            
                                            <td>@Html.DisplayFor(modelItem => item.Email)</td>
                                            <td>  @Html.DisplayFor(modelItem => item.PhoneNumber)</td>
                                            <td>Active</td>
                                           
                                        </tr>
                                       
                                    }


                                </tbody>
                            </table>
