                <div class="form-layout">
                    @using (Html.BeginForm("search", "FeedBack", FormMethod.Post, new { @class = "form-horizontal", role = "form", enctype = "multipart/form-data" }))
                    {
                        <div class="row mg-b-25">

                            <div class="col-lg-4">
                                <label class="form-control-label">From: <span class="tx-danger">*</span></label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon ion-calendar tx-16 lh-0 op-6"></i>
                                        </div>
                                    </div>
                                    <input type="text" class="form-control date hasDatepicker" name="date1" placeholder="MM-DD-YYYY" id="dp1545306018953">
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <label class="form-control-label">To : <span class="tx-danger">*</span></label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon ion-calendar tx-16 lh-0 op-6"></i>
                                        </div>
                                    </div>
                                    <input type="text" class="form-control date " name="date2" placeholder="MM-DD-YYYY" id="dp1545306018953">
                                </div>
                            </div>
                            <!-- col-4 -->
                        </div>

                        <!-- row -->
                        <div class="form-layout-footer">
                            <button class="btn btn-primary bd-0">Submit Form</button>
                            <button class="btn btn-secondary bd-0">Cancel</button>
                        </div>
                    }
                    <!-- form-layout-footer -->
                </div>

            </div>


            <div class="row row-xs mg-t-10">
                <div class="col-lg-12">
                    <div class="card card-table">
                        <div class="card-header">
                            <h6 class="slim-card-title">Ticket List</h6>
                        </div>
                        <!-- card-header -->
                        <div class="table-responsive">
                            <table class="table mg-b-0 tx-13">
                                <thead>
                                    <tr class="tx-10">
                                        <th class="wd-10p pd-y-5">S.No</th>
                                        <th class="pd-y-5">Ticket ID</th>
                                        <th class="pd-y-5">Title</th>
                                        <th class="pd-y-5">Action</th>
                                        <th class="pd-y-5">Desc.</th>
                                        <th class="pd-y-5">Status</th>


                                    </tr>
                                </thead>
                                <tbody>

                                    @foreach (var item in Model.Select((x, i) => new { Data = x, Index = i }))
                                    {

                                        <tr>
                                            <td class="pd-l-20">

                                                @(item.Index + ((Model.PageNumber - 1) * Model.PageSize) + 1)


                                            </td>
                                            <td>@Html.DisplayFor(modelitem => item.Data.id)</td>
                                            <td>

                                                @Html.DisplayFor(modelitem => item.Data.title)

                                            </td>
                                            <td>

                                                @Html.ActionLink("View", "view", new { id = item.Data.id })

                                            </td>

                                            <td>@Html.DisplayFor(modelitem => item.Data.description)</td>
                                            <td>@Html.DisplayFor(modelitem => item.Data.status)</td>

                                        </tr>

                                    }
                                </tbody>
                            </table>
                        </div>
                        <!-- table-responsive -->
                        <div class="pagination-wrapper">
                            <div class="pagination-wrapper">
                                <div id='Paging' style="text-align:center">
                                    Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber)
                                    of @Model.PageCount
                                    <ul class="pagination pagination-circle mg-b-0">

                                        <li class="page-item">
                                            @Html.PagedListPager(Model, page => Url.Action("dashboard", new { page }), new PagedListRenderOptions() { LiElementClasses = new string[] { "page-link" } })

                                        </li>

                                    </ul>
                                </div>
                            </div>
                        </div>
                        <!-- pagination-wrapper -->

